{
  "name": "babel-inline-import-loader",
  "version": "1.0.1",
  "description": "A webpack loader enabling files imported by babel-plugin-inline-import to trigger rebuilds when content changes",
  "main": "index.js",
  "repository": "git@github.com:elliottsj/babel-inline-import-loader.git",
  "author": "Spencer Elliott <me@elliottsj.com>",
  "license": "MIT",
  "scripts": {
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "jest": "jest",
    "start": "cd example && webpack-dev-server",
    "test": "npm run format:check && npm run jest"
  },
  "dependencies": {
    "@babel/parser": "^7.9.6",
    "@babel/traverse": "^7.9.6",
    "@babel/types": "^7.9.6"
  },
  "devDependencies": {
    "@babel/core": "7.10.5",
    "babel-loader": "8.1.0",
    "babel-plugin-inline-import": "3.0.0",
    "jest": "26.1.0",
    "prettier": "2.0.5",
    "webpack": "4.43.0",
